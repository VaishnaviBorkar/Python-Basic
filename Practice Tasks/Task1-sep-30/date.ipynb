      "source": [
        "# input days and convert it into date month and year\n",
        "days=int(input(\"Enter the days :\"))\n",
        "year=int(days/365)\n",
        "month=int((days-year*365)/30)\n",
        "day=int((days-year*365-month*30))\n",
        "print(\"Year=\",year,\"month=\",month,\"day=\",day)"
      ],
